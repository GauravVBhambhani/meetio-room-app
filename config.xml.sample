<?xml version='1.0' encoding='utf-8'?>
<widget id="com.room.manager" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Room Manager</name>
  <description>
    A sample Apache Cordova application that responds to the deviceready event.
  </description>
  <author email="hello@ralabs.org" href="http://cordova.io">Ralabs Team</author>
  <content src="index.html" />
  <plugin name="cordova-plugin-whitelist" spec="1" />
  <preference name="fullscreen" value="true" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <preference name="AndroidLaunchMode" value="singleTask" />
  <platform name="android">
    <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
  </platform>
  <plugin name="cordova-plugin-googleplus" source="npm">
    <variable name="REVERSED_CLIENT_ID" value="{{REVERSED_CLIENT_ID}}" />
    <variable name="WEB_APPLICATION_CLIENT_ID" value="{{WEB_APPLICATION_CLIENT_ID}}" />
  </plugin>
  <feature name="Toast">
    <param name="android-package" value="nl.xservices.plugins.Toast" />
  </feature>
  <universal-links>
    <host name="{{PROJECT_ID}}" scheme="https">
      <path url="/__/auth/handler" />
    </host>
  </universal-links>
  <icon src="res/icon/android/logo.png" />
  <engine name="android" spec="^7.0.0" />
  <plugin name="cordova-plugin-background-mode" spec="^0.7.2" />
  <plugin name="cordova-plugin-x-toast" spec="^2.6.2" />
  <plugin name="cordova-plugin-network-information" spec="^2.0.1" />
  <plugin name="cordova-plugin-android-wifi-manager" spec="^1.2.0" />
  <plugin name="cordova-plugin-brightness" spec="^0.2.0" />
  <plugin name="cordova-plugin-screen-locker" spec="^0.2.2" />
  <plugin name="cordova-plugin-camera" spec="^4.0.2" />
  <plugin name="cordova-plugin-kiosk" spec="https://github.com/hkalina/cordova-plugin-kiosk.git" />
  <plugin name="cordova-plugin-dialogs" spec="^2.0.1" />
  <plugin name="cordova-plugin-camera-preview" spec="^0.9.0" />
  <plugin name="cordova-plugin-navigationfoo" />
  <plugin name="cordova-plugin-hidden-statusfoo" />
</widget>
